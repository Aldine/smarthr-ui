{
  "name": "smarthr-ui",
  "author": "SmartHR-UI Team",
  "version": "2.1.1",
  "description": "SmartHR ui components built with React.",
  "keywords": [
    "react",
    "react-component",
    "typescript",
    "smarthr"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kufu/smarthr-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kufu/smarthr-ui/issues"
  },
  "homepage": "https://github.com/kufu/smarthr-ui#readme",
  "scripts": {
    "release": "run-s clean build",
    "build": "tsc",
    "lint:ts": "tslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint": "run-p lint:*",
    "format": "tslint --fix 'src/**/*.ts{,x}'",
    "clean": "rimraf ./lib",
    "prepublish": "yarn release",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.7",
    "@storybook/addon-links": "^4.1.7",
    "@storybook/addon-viewport": "^4.1.7",
    "@storybook/addons": "^4.1.7",
    "@storybook/cli": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "@types/lodash.isequal": "^4.5.3",
    "@types/lodash.merge": "^4.6.4",
    "@types/react": "^16.7.20",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.6",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3",
    "stylelint": "^9.10.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
